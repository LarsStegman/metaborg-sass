module Selector

imports
  Common
  
lexical syntax
  SELECTOR = [0-9a-zA-Z\[\]\=\*\^\ \-\_\$\.\#\>\+\,\~\|\:\(\)]+

lexical restrictions
  
  SELECTOR -/- [\{]

context-free start-symbols
  
  Selector
  
context-free syntax

  Selector.Arbitrary = SELECTOR

