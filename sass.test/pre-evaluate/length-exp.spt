module pre-evaluate-expression-test

language sass

fixture [[
.x {
  y: [[...]];
}
]]

test length addition [[
1px + 2px
]] run pre-evaluate-test to Length(BinExp(Add(), Int("1"), Int("2")), "px")

test length multiplication with int [[
2 * 4px
]] run pre-evaluate-test to Length(BinExp(Mul(), Int("2"), Int("4")), "px")

test length multiplication with int right [[
4px * 8
]] run pre-evaluate-test to Length(BinExp(Mul(), Int("4"), Int("8")), "px")

test length subtraction [[
4px - 8px
]] run pre-evaluate-test to Length(BinExp(Sub(), Int("4"), Int("8")), "px")
